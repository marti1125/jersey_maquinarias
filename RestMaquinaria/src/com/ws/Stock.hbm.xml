<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ws">

    <class name="Stock" table="stock">
 
        <id name="id" column="id">
            <generator class="increment" />
        </id>
 
        <property name="precio">
            <column name="precio" length="16" not-null="true" />
        </property>
 
        <property name="maquinaria">
            <column name="maquinaria" length="16" not-null="true" />
        </property>
 
        <property name="municipalidad">
            <column name="municipalidad" length="16" not-null="false" />
        </property>
 
    </class>

</hibernate-mapping>
